{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "trusted-cloud-admin",
  "description": "Trusted-Cloud AdminPanel + SystemPortal",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "OCIS"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=18.17.0",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "clean": "rimraf ./node_modules/.vite",
    "serve:trusted-cloud-public": "vite --port 3001 --mode trusted-cloud-public --host",
    "serve:trusted-cloud-private": "vite --port 3002 --mode trusted-cloud-private --host",
    "serve:trusted-cloud-system": "vite --port 3003 --mode trusted-cloud-system --host",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "build:trusted-cloud-public": "vite build --mode trusted-cloud-public",
    "build:trusted-cloud-private": "vite build --mode trusted-cloud-private",
    "build:trusted-cloud-system": "vite build --mode trusted-cloud-system",
    "preview:public": "vite preview --port 5001 --host --outDir disits-pub --mode trusted-cloud-public",
    "preview:private": "vite preview --port 5002 --host --outDir disits-pri --mode trusted-cloud-private",
    "preview:system": "vite preview --port 5003 --host --outDir disits-sys --mode trusted-cloud-system",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint && prettier \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "test:coverage": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.665.0",
    "@aws-sdk/lib-storage": "3.665.0",
    "@aws-sdk/s3-request-presigner": "3.665.0",
    "@mdi/font": "^7.2.96",
    "@types/lodash": "^4.17.7",
    "axios": "1.7.9",
    "core-js": "^3.31.0",
    "dayjs": "^1.11.6",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "qrcode.vue": "^3.4.1",
    "terser": "^5.29.1",
    "vue": "3.5.13",
    "vue-cookies": "1.8.4",
    "vue-i18n": "^9.14.2",
    "vue-router": "^4.2.2",
    "vuetify": "^3.6.8",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@rushstack/eslint-patch": "^1.3.1",
    "@tsconfig/node-lts": "^18.12.2",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.3.0",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.32.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "engined": "0.0.5",
    "engined-http": "0.0.11",
    "eslint": "^8.42.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.28.0",
    "eslint-plugin-vuetify": "^2.1.2",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.3",
    "stylelint": "^15.7.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^6.0.3",
    "typescript": "*",
    "vite": "5.3.5",
    "vite-plugin-checker": "0.6.4",
    "vite-plugin-static-copy": "2.2.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^0.32.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,yaml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "http-cache-semantics": "^4.1.1",
    "json5": "^2.2.3",
    "nanoid": "3.3.8",
    "yaml": "^2.2.2"
  },
  "stackblitz": {
    "startCommand": "yarn run test"
  }
}
